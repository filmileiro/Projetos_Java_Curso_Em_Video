import java.util.Scanner;
public class Desafio2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        
        
        for (int i = 1 ; i <= num; i++ ) { 
        	
        	i = i % num;
        	System.out.println(i);	
        	
        }
            
        sc.close();

	}

}
